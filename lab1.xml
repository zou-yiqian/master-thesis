      <item> fourth <tag>date1="fourth"; </tag> </item>
      <item> fifth <tag>date1="fifth"; </tag> </item>
      <item> sixth <tag>date1="sixth"; </tag> </item>
      <item> seventh <tag>date1="seventh"; </tag> </item>
      <item> eighth <tag>date1="eighth"; </tag> </item>
      <item> ninth <tag>date1="ninth"; </tag> </item>
      <item> tenth <tag>date1="tenth"; </tag> </item>
      <item> eleventh <tag>date1="eleventh"; </tag> </item>
      <item> twlefth <tag>date1="twlefth"; </tag> </item>
      <item> thirteenth <tag>date1="thirteenth"; </tag> </item>
      <item> fourteenth <tag>date1="fourteenth"; </tag> </item>
      <item> fifteenth <tag>date1="fifteenth"; </tag> </item>
      <item> sixteenth <tag>date1="sixteenth"; </tag> </item>
      <item> seventeenth <tag>date1="seventeenth"; </tag> </item>
      <item> eighteenth <tag>date1="eighteenth"; </tag> </item>
      <item> ninteenth <tag>date1="ninteenth"; </tag> </item>
      <item> twentieth <tag>date1="twentieth"; </tag> </item>
      <item> twenty-first <tag>date1="twenty-first"; </tag> </item>
      <item> twenty-second <tag>date1="twenty-second"; </tag> </item>
      <item> twenty-third <tag>date1="twenty-third"; </tag> </item>
      <item> twenty-forth <tag>date1="twenty-forth"; </tag> </item>
      <item> twenty-fifth <tag>date1="twenty-fifth"; </tag> </item>
      <item> twenty-sixth <tag>date1="twenty-sixth"; </tag> </item>
      <item> twenty-seventh <tag>date1="twenty-seventh"; </tag> </item>
      <item> twenty-eighth <tag>date1="twenty-eighth"; </tag> </item>
      <item> twenty-ninth <tag>date1="twenty-ninth"; </tag> </item>
      <item> thirtieth <tag>date1="thirtieth"; </tag> </item>
      <item> thirtieth-first <tag>date1="thirtieth-first"; </tag> </item>
     </one-of>
    </rule>
   </grammar>

   <filled>
    <if cond="date1=='first'">
     <goto next="#birthwrong1"/>
    <else />
     <goto next="#birthwrong2"/>
    </if>
   </filled>

   <nomatch>
    <goto next="#birthwrong2"/>
   </nomatch>
   <noinput>
    <goto next="#birthwrong2"/>
   </noinput>

  </field>
  <block>
    <goto next="#birthwrong2"/>
  </block>
 </form>
 
