<?xml version="1.0"?>
<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd"
      xmlns:voxeo="http://community.voxeo.com/xmlns/vxml">
 <var name="date1"/>
 <var name="morn1"/>
 <var name="date2"/>
 <var name="morn2"/>
 <var name="date3"/>
 <var name="morn3"/>
 <var name="date4"/>
 <var name="morn4"/>
 <var name="id" expr="Math.ceil(Math.random()*4)"/>

<!-- prompt the user what to do -->
 <form id="welcome">
  <property name="bargein" value="false"/>
  <block>
    <break time="1s" />
    <audio src="audio/instruction.wav"/>
   <if cond="id==2">
    <voxeo:recordcall value="100" info="inin"/>
    <goto next="#birthday2"/>
   <elseif cond="id==3"/>
    <voxeo:recordcall value="100" info="exex"/>
    <goto next="#date3"/>
   <elseif cond="id==4"/>
    <voxeo:recordcall value="100" info="exin"/>
    <goto next="#date4"/>
   <else/>
    <voxeo:recordcall value="100" info="inex"/>
    <goto next="#birthday1"/>
   </if>
  </block>
 </form>
 
<!-- date implicit + day explicit-->
<!-- date scenario -->
 <form id="birthday1">
  <property name="bargein" value="false"/>
  <field name="birthmonth1">
   <prompt>
      Now, let's start the <emphasis level="moderate"> first </emphasis> dialogue.
     What <emphasis level="moderate"> months? </emphasis><break time="1ms"/> is it <emphasis>today</emphasis>
 </prompt>
   <grammar mode="voice" type="application/srgs+xml" root="birthmonth1">
    <rule id="birthmonth1" scope="public">
     <one-of>
      <item> January </item>
      <item> February </item>
      <item> March </item>
      <item> April </item>
      <item> May </item>
      <item> June </item>
      <item> July </item>
      <item> August </item>
      <item> September </item>
      <item> October </item>
      <item> November </item>
      <item> December </item>
     </one-of>
    </rule>
   </grammar>
   <nomatch>
    <goto next="#birthdate1"/>
   </nomatch>
   <noinput>
    <goto next="#birthdate1"/>
   </noinput>
  </field>
  <block>
    <goto next="#birthdate1"/>
  </block>
 </form>

 <form id="birthdate1">
